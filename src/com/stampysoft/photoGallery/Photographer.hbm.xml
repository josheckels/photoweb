<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.stampysoft.photoGallery.Photographer" table="photographer">
        <id name="photographerId" column="photographer_id">
            <generator class="increment"/>
        </id>
        <property name="name"/>
        <property name="copyright"/>
    </class>

</hibernate-mapping>